{
  "name": "report-builder",
  "version": "1.0.0",
  "description": "Web application for TECH related incidents reporting.",
  "private": "true",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "pm2 start ./backend/server.js --name report-builder",
    "stop": "pm2 stop report-builder && pm2 delete all",
