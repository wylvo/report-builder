    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "msnodesqlv8": "^4.2.1",
    "mssql": "^11.0.1",
    "pm2": "^5.4.2",
    "validator": "^13.12.0",
    "winston": "^3.13.1",
